{
  "name": "react-random-numpad",
  "version": "1.0.0",
  "description": "A random genarated number pad for react",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist/ && $(npm bin)/webpack  --config webpack/production.config.js",
    "start": "start-storybook -p 8000 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "$(npm bin)/jest",
    "test:update:snapshot": "$(npm bin)/jest --updateSnapshot",
    "test:coverage": "$(npm bin)/jest --coverage",
    "lint": "$(npm bin)/tslint -c tslint.json 'src/**/*.ts*'",
    "ci": "npm run lint && npm test",
    "semantic-release": "$(npm bin)/semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nutboltu/react-random-numpad.git"
  },
  "keywords": [
    "random",
    "number",
    "pad"
  ],
  "author": "Farhad Yasir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nutboltu/react-random-numpad/issues"
  },
  "homepage": "https://github.com/nutboltu/react-random-numpad#readme",
  "peerDependencies": {
    "react": "^16.7.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-info": "^4.1.4",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/cli": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/sinon": "^7.0.2",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.6.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "husky": "^1.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.13.1",
    "sinon": "^7.2.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.5"
  }
}
